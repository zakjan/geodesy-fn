{
  "name": "geodesy-fn",
  "version": "1.2.1",
  "description": "Geodesic functions for GeoJSON, based on geodesy",
  "keywords": [
    "geodesic",
    "geodesy",
    "geojson"
  ],
  "license": "MIT",
  "author": "Jan Zak <zj@zakjan.cz>",
  "repository": "github:zakjan/geodesy-fn",
  "files": [
    "LICENSE.md",
    "dist/**/*.cjs",
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/geodesy-fn.d.ts",
      "script": "./dist/geodesy-fn.umd.min.js",
      "import": "./dist/geodesy-fn.js",
      "require": "./dist/geodesy-fn.cjs",
      "default": "./dist/geodesy-fn.js"
    }
  },
  "main": "./dist/geodesy-fn.cjs",
  "module": "./dist/geodesy-fn.js",
  "browser": "./dist/geodesy-fn.umd.min.js",
  "types": "./dist/geodesy-fn.d.ts",
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c -w",
    "test": "tsx --test src/**/*.test.ts",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/runtime": "^7.28.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/geojson": "^7946.0.16",
    "rimraf": "^6.0.1",
    "rollup": "^4.46.2",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  }
}
